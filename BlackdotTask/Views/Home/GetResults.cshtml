@{
    ViewData["Title"] = "Results";
}
@{
    List<string> data = (List<string>) ViewData["MyData"];
    var searchTerm = ViewData["Message"];
}

<h1>Combined main results from <a href="https://www.google.com/">Google</a> and <a href="https://www.searchencrypt.com/">Search Encrypt</a> for "@searchTerm"</h1>

<ul>
    @foreach (var item in @data)
    {
        <li><a href="@item">@item</a></li>
    }
</ul>

